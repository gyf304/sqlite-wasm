{
  "name": "@yifangu/sqlite",
  "version": "1.0.0-alpha.18",
  "repository": "https://github.com/gyf304/sqlite-wasm",
  "author": "Yifan Gu <gyf304@users.noreply.github.com>",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist/**/*",
    "badges/**/*",
    "README.md"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    },
    "./dist/wasm/sqlite3.wasm": "./dist/wasm/sqlite3.wasm",
    "./sqlite3.wasm": "./dist/wasm/sqlite3.wasm"
  },
  "devDependencies": {
    "@happy-dom/global-registrator": "^12.10.3",
    "@types/mocha": "^9.1.1",
    "@types/node": "^17.0.35",
    "badgen": "^3.2.2",
    "bun-types": "latest",
    "happy-dom": "^12.10.3",
    "typescript": "^4.6.4"
  },
  "scripts": {
    "compile": "make && rm -rf dist/cjs dist/esm dist/wasm && mkdir -p dist/wasm && cp sqlite/sqlite3.wasm dist/wasm/sqlite3.wasm && tsc -p ./tsconfig.dist.cjs.json && tsc -p ./tsconfig.dist.esm.json",
    "tsr": "node --loader ts-node/esm",
    "docs": "typedoc --out docs src/index.ts",
    "prepack": "bun test && bun compile && bun badgen",
    "badgen": "bun run ./scripts/badgen.ts",
    "npm": "npm"
  }
}
